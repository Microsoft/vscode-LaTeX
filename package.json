{
	"name": "latex",
	"version": "0.1.3",
	"displayName": "LaTeX Language Support",
	"description": "LaTeX - colorizer, grammar and snippets.",
	"publisher": "ms-vscode",
	"icon": "images/iconLaTeX.png",
	"license": "SEE LICENSE IN LICENSE.txt",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-LaTeX.git"
	},
	"categories": [
		"Languages",
		"Snippets"
	],
	"engines": {
		"vscode": "0.10.x"
	},
	"activationEvents": ["onLanguage:latex"],
	"main": "./out/src/latexMain",
	"contributes": {
		"languages": [
			{
				"id": "latex",
				"aliases": [
					"LaTeX",
					"latex"
				],
				"extensions": [
					".tex"
				],
				"configuration": "./latex.configuration.json"
			},
			{
				"id": "tex",
				"aliases": [
					"TeX",
					"tex"
				],
				"extensions": [
					".sty",
					".cls",
					".bbx",
					".cbx"
				],
				"configuration": "./latex.configuration.json"
			}
		],
		"snippets": [
			{
				"language": "latex",
				"path": "./snippets/snippets.json"
			}
		],
		"grammars": [
			{
				"language": "latex",
				"scopeName": "text.tex.latex",
				"path": "./syntaxes/latex.tmLanguage"
			},
			{
				"language": "tex",
				"scopeName": "text.tex",
				"path": "./syntaxes/tex.tmLanguage"
			}
		]
	},
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
	"devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
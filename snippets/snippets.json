{
	"ali": {
		"prefix": "ali",
		"body": [
			"\\begin{align}",
			"\t$0",
			"\\end{align}"
		],
		"description": "Align(ed)",
		"scope": "text.tex.latex"
	},
	"begin": {
		"prefix": "beg",
		"body": [
			"\\begin{${1:env}}",
			"\t$0",
			"\\end{$1}"
		],
		"scope": "text.tex.latex"
	},
	"figure": {
		"prefix": "fig",
		"body": [
			"\\begin{figure}",
			"\t\\centering",
			"\t$0",
			"\\end{figure}"
		],
		"scope": "text.tex.latex"
	},
	"includegraphics": {
		"prefix": "ig",
		"body": [
			"\\includegraphics[$1]{${0:path}}"
		],
		"scope": "text.tex.latex"
	},
	"frame": {
		"prefix": "frame",
		"body": [
			"\\begin{frame}",
			"\t\\frametitle{$1}",
			"\t\\framesubtitle{$2}",
			"",
			"\t$0",
			"\\end{frame}"
		],
		"scope": "text.tex.latex"
	},
	"itemize": {
		"prefix": "it",
		"body": [
			"\\begin{itemize}",
			"\t\\item $0",
			"\\end{itemize}"
		],
		"scope": "text.tex.latex"
	},
	"enumerate": {
		"prefix": "enum",
		"body": [
			"\\begin{enumerate}",
			"\t\\item $0",
			"\\end{enumerate}"
		],
		"scope": "text.tex.latex"
	},
	"description": {
		"prefix": "desc",
		"body": [
			"\\begin{description}",
			"\t\\item[$1] $0",
			"\\end{description}"
		],
		"scope": "text.tex.latex"
	},
	"emph": {
		"prefix": "e",
		"body": [
			"\\emph{$0}"
		],
		"scope": "text.tex.latex"
	},
	"textbf": {
		"prefix": "b",
		"body": [
			"{\\bfseries $0}"
		],
		"scope": "text.tex.latex"
	}
}